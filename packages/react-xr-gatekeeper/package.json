{
  "name": "react-xr-gatekeeper",
  "version": "0.0.1",
  "description": "A React hook and component to gatekeep content for VR headsets only.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["react", "webxr", "vr", "gatekeeper", "hooks"],
  "author": "WebXR Gatekeeper",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "react": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "@types/react": "catalog:",
    "@types/webxr": "catalog:",
    "vitest": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/user-event": "catalog:",
    "jsdom": "catalog:"
  }
}
